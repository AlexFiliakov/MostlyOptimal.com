# Set proper MIME types for image files
/images/*.webp
  Content-Type: image/webp
  Cache-Control: public, max-age=31536000, immutable

/images/*.gif
  Content-Type: image/gif
  Cache-Control: public, max-age=31536000, immutable

/images/*.png
  Content-Type: image/png
  Cache-Control: public, max-age=31536000, immutable

/examples/*.gif
  Content-Type: image/gif
  Cache-Control: public, max-age=31536000, immutable

# General static assets
/_next/static/*
  Cache-Control: public, max-age=31536000, immutable
